{
    "name": "backend",
    "version": "1.0.0",
    "description": "backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "tsc",
        "start": "node ./build/server.js",
        "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules ./src/server.ts",
        "prod": "tsc && node ./build/server.js",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "typeorm-ts": "ts-node ./node_modules/typeorm/cli.js",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
        "lint": "eslint . --ext .ts",
        "lint-fix": "eslint . --ext .ts --fix"
    },
    "keywords": [
        "express",
        "backend",
        "backend"
    ],
    "license": "MIT",
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.1",
        "@types/jsonwebtoken": "^8.5.4",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "formidable": "^1.2.2",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "jsonwebtoken": "^8.5.1",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.9.1",
        "mysqlm": "^2.0.3",
        "node-cron": "^3.0.0",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.0.0-pre.43",
        "typeorm": "^0.3.0-alpha.24",
        "typescript": "^3.6.3"
    },
    "_moduleAliases": {
        "@root": "src",
        "@controllers": "src/controllers",
        "@models": "src/models",
        "@util": "src/util",
        "@routes": "src/routes"
    },
    "devDependencies": {
        "@types/node": "^15.6.1",
        "@types/node-cron": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "3.2.0",
        "prettier": "^2.4.1"
    }
}
